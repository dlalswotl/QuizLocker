<?xml version="1.0" encoding="UTF-8"?>

        <PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 퀴즈 잠금화면 사용 ON/OFF 스위치 Preference -->


    <SwitchPreference android:title="퀴즈 잠금화면 사용" android:defaultValue="false" android:key="useLockScreen"/>

    <!-- 퀴즈 종류를 선택하는 MultiSelectList Preference -->


    <MultiSelectListPreference android:title="퀴즈 종류" android:defaultValue="@array/category" android:key="category" android:summary="일반상식, 역사, 수도" android:entryValues="@array/category" android:entries="@array/category"/>

</PreferenceScreen>